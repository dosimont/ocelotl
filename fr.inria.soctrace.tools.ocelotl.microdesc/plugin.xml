<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         id="idGenericTime"
         name="GenericTimeAggregationOperators"
         point="fr.inria.soctrace.tools.ocelotl.core.microscopicdescription">
      <aggregation_operator
            class="fr.inria.soctrace.tools.ocelotl.microdesc.operators.EventDistribution"
            event_category="ALL"
            generic="true"
            operator="Event Distribution - Temporal Aggregation"
            param_config="fr.inria.soctrace.tools.ocelotl.microdesc.config.DistributionConfig"
            param_win="fr.inria.soctrace.tools.ocelotl.microdesc.ui.TypeDistributionView"
            spatial_compatibility="Parts, Proportion"
            trace_formats="ALL"
            ts_default_number="100"
            value_type="Events"
            unit="events">
      </aggregation_operator>
      <aggregation_operator
            class="fr.inria.soctrace.tools.ocelotl.microdesc.operators.StateDistribution"
            event_category="STATE"
            generic="true"
            operator="State Duration - Temporal Aggregation"
            param_config="fr.inria.soctrace.tools.ocelotl.microdesc.config.DistributionConfig"
            param_win="fr.inria.soctrace.tools.ocelotl.microdesc.ui.TypeDistributionView"
            spatial_compatibility="Parts, Proportion"
            trace_formats="ALL"
            ts_default_number="100"
            value_type="States"
            unit="%TIME">
      </aggregation_operator>
      <aggregation_operator
            class="fr.inria.soctrace.tools.ocelotl.microdesc.operators.EventDistributionSpaceTime"
            event_category="ALL"
            generic="true"
            operator="Event Distribution - Spatiotemporal Aggregation"
            param_config="fr.inria.soctrace.tools.ocelotl.microdesc.config.DistributionConfig"
            param_win="fr.inria.soctrace.tools.ocelotl.microdesc.ui.HierarchicalDistributionView"
            spatial_compatibility="Matrix"
            trace_formats="ALL"
            ts_default_number="30"
            value_type="Events"
            unit="events">
      </aggregation_operator>
      <aggregation_operator
            class="fr.inria.soctrace.tools.ocelotl.microdesc.operators.StateDistributionSpaceTime"
            event_category="STATE"
            generic="true"
            operator="State Duration - Spatiotemporal Aggregation"
            param_config="fr.inria.soctrace.tools.ocelotl.microdesc.config.DistributionConfig"
            param_win="fr.inria.soctrace.tools.ocelotl.microdesc.ui.HierarchicalDistributionView"
            spatial_compatibility="Matrix, SMode"
            trace_formats="ALL"
            ts_default_number="30"
            value_type="States"
            unit="%TIME">
      </aggregation_operator>
            <aggregation_operator
            class="fr.inria.soctrace.tools.ocelotl.microdesc.operators.VariableDistribution"
            event_category="VARIABLE"
            generic="true"
            operator="Variable Mean - Temporal Aggregation"
            param_config="fr.inria.soctrace.tools.ocelotl.microdesc.config.DistributionConfig"
            param_win="fr.inria.soctrace.tools.ocelotl.microdesc.ui.TypeDistributionView"
            spatial_compatibility="Parts, Proportion"
            trace_formats="ALL"
            ts_default_number="100"
            value_type="Variables"
            unit="%UNKNOWN">
      </aggregation_operator>
            <aggregation_operator
            class="fr.inria.soctrace.tools.ocelotl.microdesc.operators.VariableDistributionSpaceTime"
            event_category="VARIABLE"
            generic="true"
            operator="Variable Mean - Spatiotemporal Aggregation"
            param_config="fr.inria.soctrace.tools.ocelotl.microdesc.config.DistributionConfig"
            param_win="fr.inria.soctrace.tools.ocelotl.microdesc.ui.HierarchicalDistributionView"
            spatial_compatibility="Matrix"
            trace_formats="ALL"
            ts_default_number="30"
            value_type="Variables"
            unit="%UNKNOWN">
      </aggregation_operator>
   </extension>
   
   <extension
         id="idGenericMicroModel"
         name="MicroModel"
         point="fr.inria.soctrace.tools.ocelotl.core.microscopicmodel">
     <microscopic_model
     		name="Events"
            type="ALL">
      </microscopic_model>
      <microscopic_model
      		name="States"
            type="STATE">
      </microscopic_model>
      <microscopic_model
     		name="Variables"
            type="VARIABLE">
      </microscopic_model>
   </extension>
       
</plugin>
