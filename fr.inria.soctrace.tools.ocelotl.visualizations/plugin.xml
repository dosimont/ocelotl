<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         id="idGenericSpace"
         name="GenericSpaceAggregationOperators"
         point="fr.inria.soctrace.tools.ocelotl.core.visualization">
      <visualization_operator
            class="fr.inria.soctrace.tools.ocelotl.visualizations.parts.Parts"
            operator="Parts"
            param_config="fr.inria.soctrace.tools.ocelotl.visualizations.parts.config.PartsConfig"
            param_win="fr.inria.soctrace.tools.ocelotl.visualizations.parts.config.PartsConfigView"
            metric_compatibility="PARTS"
            aggregator_compatibility="2D"
            selection_priority="4"
            visualization="fr.inria.soctrace.tools.ocelotl.visualizations.parts.views.PartTimeLineView">
      </visualization_operator>
      <visualization_operator
            class="fr.inria.soctrace.tools.ocelotl.visualizations.proportion.Proportion"
            operator="Proportion"
            param_config="fr.inria.soctrace.tools.ocelotl.visualizations.proportion.config.ProportionConfig"
            param_win="fr.inria.soctrace.tools.ocelotl.visualizations.proportion.config.ManageColorsDialog"
            metric_compatibility="PROPORTIONS"
            aggregator_compatibility="2D"
            selection_priority="3"
            visualization="fr.inria.soctrace.tools.ocelotl.visualizations.proportion.views.StateProportionTimeLineView">
      </visualization_operator>
      <visualization_operator
            class="fr.inria.soctrace.tools.ocelotl.visualizations.matrixparts.MatrixParts"
            operator="Matrix"
            metric_compatibility="PARTS"
            aggregator_compatibility="3D"
            selection_priority="2"
            visualization="fr.inria.soctrace.tools.ocelotl.visualizations.matrixparts.views.PartMatrixView">
      </visualization_operator>
      <visualization_operator
            class="fr.inria.soctrace.tools.ocelotl.visualizations.mode.MatrixProportion"
            operator="Mode"
            param_config="fr.inria.soctrace.tools.ocelotl.visualizations.proportion.config.ProportionConfig"
            param_win="fr.inria.soctrace.tools.ocelotl.visualizations.proportion.config.ManageColorsDialog"
            metric_compatibility="MODE"
            aggregator_compatibility="3D"
            selection_priority="1"
            visualization="fr.inria.soctrace.tools.ocelotl.visualizations.mode.views.ProportionMatrixView">
      </visualization_operator>
   </extension>
</plugin>
