<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         id="idGenericSpace"
         name="GenericSpaceAggregationOperators"
         point="fr.inria.soctrace.tools.ocelotl.core.visualization">
      <visualization_operator
            class="fr.inria.soctrace.tools.ocelotl.visualizations.temporal.partition.TemporalPartition"
            operator="Temporal Partition"
            param_config="fr.inria.soctrace.tools.ocelotl.visualizations.config.temporalpartition.PartitionConfig"
            param_win="fr.inria.soctrace.tools.ocelotl.visualizations.config.temporalpartition.PartitionConfigView"
            metric_compatibility="PARTS"
            aggregator_compatibility="2D"
            selection_priority="4"
            overview_visualization="Temporal Mode"
            visualization="fr.inria.soctrace.tools.ocelotl.visualizations.temporal.partition.views.TemporalPartitionView">
      </visualization_operator>
      <visualization_operator
            class="fr.inria.soctrace.tools.ocelotl.visualizations.temporal.proportion.TemporalProportion"
            operator="Temporal Stacked Bar Chart"
            param_config="fr.inria.soctrace.tools.ocelotl.visualizations.config.eventcolor.ColorsConfig"
            param_win="fr.inria.soctrace.tools.ocelotl.visualizations.config.eventcolor.ManageColorsDialog"
            metric_compatibility="PROPORTIONS"
            aggregator_compatibility="2D"
            selection_priority="3"
            overview_visualization="Temporal Mode"
            visualization="fr.inria.soctrace.tools.ocelotl.visualizations.temporal.proportion.views.TemporalProportionView">
      </visualization_operator>
      <visualization_operator
            class="fr.inria.soctrace.tools.ocelotl.visualizations.spatiotemporal.partition.SpatiotemporalPartition"
            operator="Spatiotemporal Partition"
            metric_compatibility="PARTS"
            param_config=""
            param_win=""
            aggregator_compatibility="3D"
            selection_priority="2"
            overview_visualization="Temporal Mode"
            visualization="fr.inria.soctrace.tools.ocelotl.visualizations.spatiotemporal.partition.views.SpatiotemporalPartitionView">
      </visualization_operator>
      <visualization_operator
            class="fr.inria.soctrace.tools.ocelotl.visualizations.spatiotemporal.mode.SpatiotemporalMode"
            operator="Spatiotemporal Mode"
            param_config="fr.inria.soctrace.tools.ocelotl.visualizations.config.eventcolor.ColorsConfig"
            param_win="fr.inria.soctrace.tools.ocelotl.visualizations.config.eventcolor.ManageColorsDialog"
            metric_compatibility="MODE"
            aggregator_compatibility="3D"
            selection_priority="1"
            overview_visualization="Temporal Mode"
            visualization="fr.inria.soctrace.tools.ocelotl.visualizations.spatiotemporal.mode.views.SpatiotemporalModeView">
      </visualization_operator>
  	  <visualization_operator
            class="fr.inria.soctrace.tools.ocelotl.visualizations.temporal.mode.TemporalMode"
            operator="Temporal Mode"
            param_config="fr.inria.soctrace.tools.ocelotl.visualizations.config.eventcolor.ColorsConfig"
            param_win="fr.inria.soctrace.tools.ocelotl.visualizations.config.eventcolor.ManageColorsDialog"
  			metric_compatibility="MODE"
            aggregator_compatibility="2D"
            selection_priority="5"
            overview_visualization="Temporal Mode"
            visualization="fr.inria.soctrace.tools.ocelotl.visualizations.temporal.mode.views.TemporalModeView">
      </visualization_operator>
   </extension>
</plugin>
